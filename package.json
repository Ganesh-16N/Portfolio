{
  "name": "ganesh-nagargoje-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Senior React Native Developer Portfolio - Ganesh Nagargoje",
  "keywords": [
    "react-native",
    "mobile-development",
    "portfolio",
    "senior-developer"
  ],
  "author": "Ganesh Nagargoje",
  "license": "MIT",
  "dependencies": {
    "framer-motion": "^12.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "npm run build && vercel --prod",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "optimize": "node scripts/optimize.js",
    "performance": "npm run optimize && npm run build",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "audit": "npm audit && npm run optimize"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
